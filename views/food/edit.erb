
<h1>Edit <%=@food.name%></h1>


	<%= form_open(@food, method: 'patch') %>
	<p>Food Name:</p><%=input_tag(type: 'text', resource: 'food', name: 'name', value: @food.name) %>
	<p>Cuisine:</p><%=input_tag(type: 'text', resource: 'food', name: 'cuisine', value: @food.cuisine) %>
	<p>Price($):</p><%=input_tag(type: 'text', resource: 'food', name: 'price', value: @food.price) %>
	<%=input_tag(type: 'submit', value: "Update Item") %>
	<%=form_close %>